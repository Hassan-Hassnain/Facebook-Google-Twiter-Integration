# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'FaceBook Login Integration' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FaceBook Login Integration
    pod 'FBSDKCoreKit'
    pod 'FBSDKLoginKit'
    pod 'FBSDKShareKit'
    
    
    pod 'FacebookCore'
    pod 'FacebookLogin'
    pod 'FacebookShare'
    
    
    pod 'GoogleSignIn'
    
end
